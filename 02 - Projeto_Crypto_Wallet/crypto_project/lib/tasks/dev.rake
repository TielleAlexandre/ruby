namespace :dev do
  #cria as tabelas, migra os dados e cadastra as moedas presentes em db/seeds.rb
  desc "Configurando o ambiente de desenvolvimento"
  task setup: :environment do
    if Rails.env.development
      puts %x(rails db:drop db:create db:migrate db:seed)
    end
  end

  desc "Cadastra as moedas"
  task add_coins: :environment do

    coins = [
      { description: "Homes",
        acronym: "hm",
        url_image: "https://upload.wikimedia.org/wikipedia/pt/0/02/Homer_Simpson_2006.png",
        mining_type: MiningType.find_by(acronym: "PoW")
      },
      {
        description: "Pudim",
        acronym: "Puds",
        url_image: "https://blogger.googleusercontent.com/img/b/R29vZ2xl/AVvXsEg1CJV5Xfhg2bT2nlEsGgGr26ffkl3lMdus7wIo3lbwXl6QZ2MkCRhbex3bO2mqfyHEtO9i9QtLCGDusXSPqxvDS6lGs8Cjeru0Sj5C_p0RIs7q6K9USDBPftqxOkBKMpMawKDP7ar6qkZ8OWWdZ0ksZy_4Przy9dPAZXlBddqr6Fc9xkwGJ55ZYfhY/s1024/Sonic%20imagem%20png.png",
        mining_type: MiningType.all.sample
      },
      {
        description: "Brunis",
        acronym: "Bru",
        url_image: "https://i.pinimg.com/originals/60/cd/ff/60cdff33a7c7d255fb8ec3c153f4ccd8.png",
        mining_type: MiningType.find_by(acronym: "PoW")
      },
      {
        description: "Naza",
        acronym: "Nazs",
        url_image: "data:image/jpeg;base64,",
        mining_type: MiningType.all.sample
      }
    ]

    coins.each do |coin|
      Coin.find_or_create_by!(coin)
    end
  end


  desc "Cadastra os tipos de mineração"
  task add_minings_types: :environment do

    minings = [
      {  description: "Proof of Work",
         acronym: "PoW",
      },
      {  description: "Proof of Stake",
         acronym: "PoS",
      },
      {  description: "Proof of Capacity",
         acronym: "PoC",
      }
    ]

    minings.each do |mining|
      MiningType.find_or_create_by!(mining)
    end
  end

  desc "Cadastra os dados no banco de dados"
  task inserts: :environment do
    puts "Cadastrando os dados no banco de dados..."
      %x(rails dev:add_minings_types)
      %x(rails dev:add_coins)
    end
end
